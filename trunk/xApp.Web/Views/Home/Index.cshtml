<!doctype html />
<html>
<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="/static/css/bootstrap.min.css" />
    <link rel="stylesheet" href="/static/css/bootstrap-responsive.min.css" />
    <link href="/static/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <link href="~/Static/css/matrix-style.css" rel="stylesheet" />
    <link href="~/Static/css/matrix-media.css" rel="stylesheet" />
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,700,800' rel='stylesheet' type='text/css'>

    <script src="~/Static/lib/jquery.min.js"></script>
    <script src="/static/lib/angular.js" type="text/javascript"></script>
    <script src="/static/lib/angular-resource.js" type="text/javascript"></script>
    <script src="~/Static/lib/bootstrap.min.js"></script>
    <title></title>
</head>
<body ng-controller="ctrl">

    <!--Header-part-->
    <div id="header">
        <h1><a href="dashboard.html">Matrix Admin</a></h1>
    </div>
    <!--close-Header-part-->

    <!--top-Header-menu-->
    <div id="user-nav" class="navbar navbar-inverse">
        <ul class="nav">
            <li class="dropdown" id="profile-messages"><a title="" href="#" data-toggle="dropdown" data-target="#profile-messages" class="dropdown-toggle"><i class="icon icon-user"></i><span class="text">Welcome User</span><b class="caret"></b></a>
                <ul class="dropdown-menu">
                    <li><a href="#"><i class="icon-user"></i>My Profile</a></li>
                    <li class="divider"></li>
                    <li><a href="#"><i class="icon-check"></i>My Tasks</a></li>
                    <li class="divider"></li>
                    <li><a href="login.html"><i class="icon-key"></i>Log Out</a></li>
                </ul>
            </li>
            <li class="dropdown" id="menu-messages"><a href="#" data-toggle="dropdown" data-target="#menu-messages" class="dropdown-toggle"><i class="icon icon-envelope"></i><span class="text">Messages</span> <span class="label label-important">5</span> <b class="caret"></b></a>
                <ul class="dropdown-menu">
                    <li><a class="sAdd" title="" href="#"><i class="icon-plus"></i>new message</a></li>
                    <li class="divider"></li>
                    <li><a class="sInbox" title="" href="#"><i class="icon-envelope"></i>inbox</a></li>
                    <li class="divider"></li>
                    <li><a class="sOutbox" title="" href="#"><i class="icon-arrow-up"></i>outbox</a></li>
                    <li class="divider"></li>
                    <li><a class="sTrash" title="" href="#"><i class="icon-trash"></i>trash</a></li>
                </ul>
            </li>
            <li class=""><a title="" href="#"><i class="icon icon-cog"></i><span class="text">Settings</span></a></li>
            <li class=""><a title="" href="login.html"><i class="icon icon-share-alt"></i><span class="text">Logout</span></a></li>
        </ul>
    </div>
    <!--close-top-Header-menu-->

    <!--start-top-serch-->
    <div id="search">
        <input type="text" placeholder="Search here..." />
        <button type="submit" class="tip-bottom" title="Search"><i class="icon-search icon-white"></i></button>
    </div>
    <!--close-top-serch-->

    <!--sidebar-menu-->
    <div accordion on-click="menuClick(item)" id="sidebar"></div>

    <!--close-sidebar-menu-->

    <!--main-container-part-->
    <div id="content">
        <div id="content-header">
            <tabset>
                <tab ng-repeat="tab in tabs" active="tab.active" disable="tab.disable">
                    <tab-heading>
                        <i class="icon-home">
                            {{tab.title}} 
                            <button class="close" ng-click="closeTab(tab)" type="button" title="Remove this page">×</button>
                        </i>
                    </tab-heading>
                    <iframe src="/static/index.html" frameborder="0" border="0" 
                        marginwidth="0" marginheight="0" scrolling="yes" width="100%" 
                        height="100%"></iframe>
                </tab>
            </tabset>
        </div>
    </div>

    <script src="~/Static/lib/ui-bootstrap-tpls-0.7.0.js"></script>

    @*    <script src="~/Static/app/services/core.service.js"></script>
    <script src="~/Static/app/controllers/index.controller.js"></script>
    <script>
        angular.bootstrap(document.body, ['app']);
    </script>*@

    <script src="~/static/lib/require.js" data-main="/static/app/main"></script>
</body>
</html>
